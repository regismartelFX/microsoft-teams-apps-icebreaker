{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.3/MicrosoftTeams.schema.json",
  "manifestVersion": "1.3",
  "version": "1.0.0",
  "id": "d55d08d9-bacb-4567-942f-afd85d632dcc",
  "packageName": "com.microsoft.teams.icebreaker",
  "developer": {
    "name": "FX Innovation",
    "websiteUrl": "https://www.fxinnovation.com",
    "privacyUrl": "https://www.fxinnovation.com",
    "termsOfUseUrl": "https://www.fxinnovation.com"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "FX Coffee Machine"
  },
  "description": {
    "short": "FX Coffee Machine is a bot that randomly pairs team members up.",
    "full": "FX Coffee Machine is a cute little bot that randomly pairs team members up every week to help the team build trust and personal connections."
  },
  "accentColor": "#64A2CC",
  "bots": [
    {
      "botId": "<bot id>",
      "scopes": [
        "personal",
        "team"
      ],
      "supportsFiles": false,
      "isNotificationOnly": true
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<app domain>"
  ]
}